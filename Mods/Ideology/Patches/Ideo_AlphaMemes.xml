			<li>Alpha Memes</li>
		</mods> 
				
		<match Class="PatchOperationSequence">
			<operations>
				
				<li Class="PatchOperationAdd">
					<xpath>/Defs/MemeDef[defName = "MorrowRim_BonewalkerSupremacy"]</xpath>
